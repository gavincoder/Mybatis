<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.mybatis.cache.dao.mapper.TruckInfoMapper">
    <select id="getTruckInfo" parameterType="java.lang.String"
            resultType="com.example.mybatis.cache.dao.entity.TruckInfo">
        select * from `tbl_truck_info` where truck_no = #{truckNo}
    </select>


</mapper>